extends Node

signal console_log(message)

var rng: RandomNumberGenerator

###############################################################################
# Builtin functions                                                           #
###############################################################################

func _ready() -> void:
	rng =  RandomNumberGenerator.new()
	rng.randomize()

###############################################################################
# Connections                                                                 #
###############################################################################

###############################################################################
# Private functions                                                           #
###############################################################################

###############################################################################
# Public functions                                                            #
###############################################################################

func console_log(message: String) -> void:
	print(message)
	emit_signal("console_log", message)
